    <!-- localehelp activity title -->
    <string name="locale_title">設定</string>

    <!-- localehelp activity items -->
    <string-array name="locale_items">
        <item>繁體中文</item>
        <item>英文</item>
    </string-array>

    <!-- localehelp activity icon -->
    <string-array name="locale_icons">
        <item>@drawable/icon_tw</item>
