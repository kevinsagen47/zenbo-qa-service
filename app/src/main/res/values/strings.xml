<resources>
    <!-- app name -->
    <string name="app_name">圖吱小獼</string>

    <!-- main activity title -->
    <string name="main_title">圖資處服務資訊</string>

    <!-- main activity items -->
    <string-array name="main_items">
        <item>常用問題</item>
        <item>問題服務</item>
        <item>服務導覽</item>
        <item>館藏查詢</item>
        <item>資料庫查詢</item>
